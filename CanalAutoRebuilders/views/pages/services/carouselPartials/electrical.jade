    li(data-target='#electrical-carousel', data-slide-to='5')
    li(data-target='#electrical-carousel', data-slide-to='6')
    li(data-target='#electrical-carousel', data-slide-to='7')
    li(data-target='#electrical-carousel', data-slide-to='8')
    li(data-target='#electrical-carousel', data-slide-to='9')
  .carousel-inner(role='listbox')
    .item.active
      img(src='images/Services/electricalAndInterior/electricalAndInterior-1.jpg', alt='...')
