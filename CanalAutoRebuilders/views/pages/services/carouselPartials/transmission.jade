#transmission-carousel.carousel.slide(data-ride='carousel')
  ol.carousel-indicators
    li.active(data-target='#transmission-carousel', data-slide-to='0')
  .carousel-inner(role='listbox')
    .item.active
      img(src='images/Services/Transportation/transportation-1.jpg', alt='...')
  a.left.carousel-control(href='#transmission-carousel', role='button', data-slide='prev')
    span.glyphicon.glyphicon-chevron-left(aria-hidden='true')
    span.sr-only Previous
  a.right.carousel-control(href='#transmission-carousel', role='button', data-slide='next')
    span.glyphicon.glyphicon-chevron-right(aria-hidden='true')
    span.sr-only Next
