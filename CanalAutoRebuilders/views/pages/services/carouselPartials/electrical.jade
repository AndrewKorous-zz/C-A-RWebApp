    li.active(data-target='#electrical-carousel', data-slide-to='0')
    li(data-target='#electrical-carousel', data-slide-to='1')
    li(data-target='#electrical-carousel', data-slide-to='2')
    li(data-target='#electrical-carousel', data-slide-to='3')
