#engine-detail-carousel.carousel.slide(data-ride='carousel')
  ol.carousel-indicators
    li.active(data-target='#engine-detail-carousel', data-slide-to='0')
    li(data-target='#engine-detail-carousel', data-slide-to='1')
    li(data-target='#engine-detail-carousel', data-slide-to='2')
    li(data-target='#engine-detail-carousel', data-slide-to='3')
    li(data-target='#engine-detail-carousel', data-slide-to='4')
    li(data-target='#engine-detail-carousel', data-slide-to='5')
    li(data-target='#engine-detail-carousel', data-slide-to='6')
    li(data-target='#engine-detail-carousel', data-slide-to='7')
    li(data-target='#engine-detail-carousel', data-slide-to='8')
    li(data-target='#engine-detail-carousel', data-slide-to='9')
  .carousel-inner(role='listbox')
    .item.active
      img(src='images/Services/EngineDetail/engineDetail-1.jpg', alt='...')
    .item
      img(src='images/Services/EngineDetail/engineDetail-2.jpg', alt='...')
    .item
      img(src='images/Services/EngineDetail/engineDetail-3.jpg', alt='...')
    .item
      img(src='images/Services/EngineDetail/engineDetail-4.jpg', alt='...')
    .item
      img(src='images/Services/EngineDetail/engineDetail-5.jpg', alt='...')
    .item
      img(src='images/Services/EngineDetail/engineDetail-6.jpg', alt='...')
    .item
      img(src='images/Services/EngineDetail/engineDetail-7.jpg', alt='...')
    .item
      img(src='images/Services/EngineDetail/engineDetail-8.jpg', alt='...')
    .item
      img(src='images/Services/EngineDetail/engineDetail-9.jpg', alt='...')
    .item
      img(src='images/Services/EngineDetail/engineDetail-10.jpg', alt='...')
  a.left.carousel-control(href='#engine-detail-carousel', role='button', data-slide='prev')
    span.glyphicon.glyphicon-chevron-left(aria-hidden='true')
    span.sr-only Previous
  a.right.carousel-control(href='#engine-detail-carousel', role='button', data-slide='next')
    span.glyphicon.glyphicon-chevron-right(aria-hidden='true')
    span.sr-only Next
