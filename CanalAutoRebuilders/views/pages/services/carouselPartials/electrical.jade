      img(src='images/Services/electricalAndInterior/electricalAndInterior-6.jpg', alt='...')
    .item
      img(src='images/Services/electricalAndInterior/electricalAndInterior-7.jpg', alt='...')
    .item
      img(src='images/Services/electricalAndInterior/electricalAndInterior-8.jpg', alt='...')
    .item
      img(src='images/Services/electricalAndInterior/electricalAndInterior-9.jpg', alt='...')
    .item
      img(src='images/Services/electricalAndInterior/electricalAndInterior-10.jpg', alt='...')
  a.left.carousel-control(href='#electrical-carousel', role='button', data-slide='prev')
    span.glyphicon.glyphicon-chevron-left(aria-hidden='true')
    span.sr-only Previous
  a.right.carousel-control(href='#electrical-carousel', role='button', data-slide='next')
    span.glyphicon.glyphicon-chevron-right(aria-hidden='true')
    span.sr-only Next
