#suspension-brakes-carousel.carousel.slide(data-ride='carousel')
  ol.carousel-indicators
    li.active(data-target='#suspension-brakes-carousel', data-slide-to='0')
    li(data-target='#suspension-brakes-carousel', data-slide-to='1')
    li(data-target='#suspension-brakes-carousel', data-slide-to='2')
    li(data-target='#suspension-brakes-carousel', data-slide-to='3')
  .carousel-inner(role='listbox')
    .item.active
      img(src='images/Services/suspensionAndBreaks/suspensionAndBreaks-1.jpg', alt='...')
    .item
      img(src='images/Services/suspensionAndBreaks/suspensionAndBreaks-2.jpg', alt='...')
    .item
      img(src='images/Services/suspensionAndBreaks/suspensionAndBreaks-3.jpg', alt='...')
    .item
      img(src='images/Services/suspensionAndBreaks/suspensionAndBreaks-4.jpg', alt='...')
  a.left.carousel-control(href='#suspension-brakes-carousel', role='button', data-slide='prev')
    span.glyphicon.glyphicon-chevron-left(aria-hidden='true')
    span.sr-only Previous
  a.right.carousel-control(href='#suspension-brakes-carousel', role='button', data-slide='next')
    span.glyphicon.glyphicon-chevron-right(aria-hidden='true')
    span.sr-only Next
